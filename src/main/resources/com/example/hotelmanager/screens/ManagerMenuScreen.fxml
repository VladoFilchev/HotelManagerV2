<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="401.0"
            stylesheets="@../CSS/ManagerMenu.css"
            xmlns="http://javafx.com/javafx/23.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controllers.ManagerMenuController">
    <children>
        <Pane fx:id="paneObj" prefHeight="87.0" prefWidth="401.0">
            <children>
                <Text fx:id="greetingText" fill="WHITE" layoutX="130.0" layoutY="30.0"
                      strokeType="OUTSIDE" strokeWidth="0.0"
                      textAlignment="CENTER" wrappingWidth="150.0"
                      text="Welcome Manager">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Text>
                <Text fill="WHITE" layoutX="135.0" layoutY="54.0"
                      strokeType="OUTSIDE" strokeWidth="0.0"
                      text="Manager Menu">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Text>
            </children>
        </Pane>
        <Button fx:id="registerReceptionistButton"
                onAction="#openReceptionistRegistration"
                layoutX="100.0" layoutY="207.0"
                mnemonicParsing="false"
                prefHeight="51.0" prefWidth="200.0"
                text="Register Receptionist"
                AnchorPane.leftAnchor="100.0"
                AnchorPane.rightAnchor="100.0"/>
    </children>
</AnchorPane>
